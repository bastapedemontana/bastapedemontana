---
layout: default
---
<article class="post">

  <header class="post-header">
    <h1 class="post-title" style="display: inline-block; padding: 40px 0 20px 0;">{{ page.title | escape }}</h1>
  <img style="max-width: 40%; max-height: 150px; float: right;" src="/assets/protesta.png" />
  </header>

  <div class="post-content">
    # {{ content }}
    {{ post.content | remove: post.excerpt }}
  </div>

</article>
